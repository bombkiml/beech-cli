{
    "name": "bombkiml/beech-cli",
	"description": "PHP Beech command line interface. (CLI)",
    "keywords": ["beech", "beech-cli", "php-cli", "cli", "phpbeech", "cli-framework"],
    "license": "MIT",
    "authors": [
        {
            "name": "bombkiml",
            "email": "nattapat.jquery@gmail.com",
            "homepage": "https://facebook.com/bombkiml",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0 || ^7.0"
    },
    "autoload": {
        "psr-0": {
            "bombkiml/beech-cli": "src",
            "xx": "lib"
        }
    },
    "extra": {
        "installer-paths": {
            "./bombkiml/": ["bombkiml/beech-cli"]
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php beech init"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
